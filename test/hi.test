  org 2
  hello: word: 12, 'Hello world!'
  addr: word: hello
  cnt: word: 0
  out: word: 2047

  org 25
  start: load hello
      store cnt
      loop: load addr
          inc
          store addr
          load (addr)
          store (out)
          load cnt
          dec
          store cnt
          jmz end
          jmp loop
      end: hlt